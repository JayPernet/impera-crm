    "animations": {
        "philosophy": "Minimal e purposeful. Animações devem comunicar estado/transição, não decorar. Velocidade rápida para não atrasar o workflow.",
        "durations": {
            "instant": "50ms",
            "fast": "150ms",
            "normal": "250ms",
            "slow": "400ms"
        },
        "easings": {
            "default": "cubic-bezier(0.4, 0, 0.2, 1)",
            "in": "cubic-bezier(0.4, 0, 1, 1)",
            "out": "cubic-bezier(0, 0, 0.2, 1)",
            "inOut": "cubic-bezier(0.4, 0, 0.2, 1)"
        },
        "types": {
            "fadeIn": {
                "initial": "opacity: 0",
                "animate": "opacity: 1",
                "duration": "150ms",
                "easing": "ease-out"
            },
            "slideDown": {
                "initial": "opacity: 0, translateY: -8px",
                "animate": "opacity: 1, translateY: 0",
                "duration": "200ms",
                "easing": "cubic-bezier(0.4, 0, 0.2, 1)"
            },
            "scaleIn": {
                "initial": "opacity: 0, scale: 0.95",
                "animate": "opacity: 1, scale: 1",
                "duration": "150ms",
                "easing": "cubic-bezier(0.4, 0, 0.2, 1)"
            }
        },
        "microInteractions": {
            "buttonPress": "scale(0.98) on active, duration 50ms",
            "hoverLift": "none - não usamos lift em dark mode operacional",
            "focusRing": "0 0 0 3px rgba(primary, 0.12), duration 150ms"
        }
    },
    "layout": {
        "philosophy": "Information-dense mas escaneável. Sidebar fixa, conteúdo full-width, hierarquia visual clara por contraste e spacing.",
        "structure": {
            "sidebar": {
                "width": "240px",
                "background": "#0E1116",
                "border": "1px solid #232A34 (right)",
                "position": "fixed"
            },
            "header": {
                "height": "64px",
                "background": "#151A21",
                "border": "1px solid #232A34 (bottom)",
                "padding": "0 24px"
            },
            "mainContent": {
                "marginLeft": "240px",
                "padding": "24px",
                "background": "#0E1116"
            }
        },
        "grid": {
            "dashboard": "2-column em desktop, 1-column em mobile. Cards de KPIs 4-across, depois 2-across para gráficos",
            "tables": "Full-width com scroll horizontal se necessário",
            "forms": "Single column max-width 600px, 2-column em campos relacionados (nome/sobrenome)"
        }
